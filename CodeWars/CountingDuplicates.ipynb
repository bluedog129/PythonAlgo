    "text = 'abcdeaB'\n",
    "\n",
    "def duplicate_count(text):\n",
    "    lowertext = text.lower()\n",
    "    found = []\n",
    "    \n",
    "    for char in lowertext:\n",
    "        if(not(char in found) and lowertext.count(char) > 1):\n",
    "            found.append(char)\n",
    "    \n",
    "    return len(found)\n",
    "    \n",
    "duplicate_count(text)"
   ]
