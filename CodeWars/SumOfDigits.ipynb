    "n = 132189\n",
    "\n",
    "def digit_sum(n):\n",
    "    if n<10:\n",
    "        return n\n",
    "    return n%10 + digit_sum(n//10)\n",
    "\n",
    "def digital_root(n):\n",
    "    while n>9:\n",
    "        n = digit_sum(n)\n",
    "    return n\n",
    "\n",
    "digital_root(n)"
   ]
