   "source": [
    "absolutes = [4,7,12]\n",
    "signs = [True,False,True]\n",
    "\n",
    "def solution(absolutes, signs):\n",
    "    answer = 0\n",
    "    \n",
    "    for i in zip(absolutes, signs):\n",
    "        if i[1] == True:\n",
    "            answer += i[0]\n",
    "        else:\n",
    "            answer -= i[0]\n",
    "    \n",
    "    return answer\n",
    "\n",
    "solution(absolutes, signs)"
   ]
