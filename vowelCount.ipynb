    "input_str = \"abracadabra\"\n",
    "\n",
    "def get_count(input_str):\n",
    "    num_vowels = 0\n",
    "    \n",
    "    vowels = set(\"aeiou\")\n",
    "    \n",
    "    for letter in input_str:\n",
    "        if letter in vowels:\n",
    "            num_vowels += 1\n",
    "            \n",
    "    return num_vowels\n",
    "\n",
    "get_count(input_str)"
   ]
